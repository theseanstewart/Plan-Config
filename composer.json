{
    "name": "seanstewart/plan-config",
    "description": "Plan config allows you to easily define attributes and limits for your SaaS application subscription plans.",
    "keywords": ["subscription plans", "saas", "plans", "config", "laravel", "laravel5"],
    "homepage": "https://github.com/theseanstewart/Plan-Config",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Stewart",
            "email": "sean@ekoim.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Seanstewart\\PlanConfig\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seanstewart\\PlanConfig\\PlanConfigServiceProvider"
            ],
            "aliases": {
                "Plan": "Seanstewart\\PlanConfig\\Plan"
            }
        }
    },
    "minimum-stability": "stable"
}
